{
  "name": "JellyWatch",
  "target_url": "http://localhost:8080/api/v1/webhooks/jellyfin",
  "method": "POST",
  "content_type": "application/json",
  "headers": {
    "X-Jellywatch-Webhook-Secret": "YOUR_SHARED_SECRET"
  },
  "events": [
    "ItemAdded",
    "PlaybackStart",
    "PlaybackStop",
    "TaskCompleted",
    "LibraryChanged"
  ],
  "payload_template": {
    "NotificationType": "{{NotificationType}}",
    "ServerName": "{{ServerName}}",
    "ServerId": "{{ServerId}}",
    "ItemId": "{{ItemId}}",
    "Name": "{{Name}}",
    "ItemType": "{{ItemType}}",
    "ItemPath": "{{ItemPath}}",
    "Year": "{{Year}}",
    "SeriesName": "{{SeriesName}}",
    "SeasonNumber": "{{SeasonNumber}}",
    "EpisodeNumber": "{{EpisodeNumber}}",
    "Provider_tmdb": "{{Provider_tmdb}}",
    "Provider_tvdb": "{{Provider_tvdb}}",
    "Provider_imdb": "{{Provider_imdb}}",
    "NotificationUsername": "{{NotificationUsername}}",
    "DeviceName": "{{DeviceName}}",
    "ClientName": "{{ClientName}}",
    "TaskName": "{{TaskName}}",
    "Timestamp": "{{Timestamp}}"
  }
}
